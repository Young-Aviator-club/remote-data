{
  "name": "@young-aviator-club/remote-data",
  "source": "src/index.ts",
  "version": "2.1.1",
  "private": false,
  "description": "Typesafe library for representation loadable state.",
  "exports": {
    "require": "./dist/index.js",
    "default": "./dist/index.module.js"
  },
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "unpkg": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/Young-Aviator-club/remote-data"
  },
  "scripts": {
    "type-check": "tsc --noEmit --p tsconfig.build.json",
    "build": "npm run clean && microbundle",
    "test": "jest",
    "test:coverage": "jest --collect-coverage --silent",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --quiet",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run lint && npm run test && npm run build"
  },
  "files": [
    "/dist",
    "/package.json"
  ],
  "keywords": [
    "typescript",
    "javascript",
    "typesafe",
    "remote-data",
    "fp",
    "state",
    "data structures",
    "fetch",
    "types",
    "react",
    "redux",
    "rtk",
    "immer"
  ],
  "author": "Kalagin Ivan",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.2",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "babel-jest": "^29.5.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.1.7",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.9.1",
    "expect-type": "^0.15.0",
    "jest": "^29.5.0",
    "microbundle": "^0.15.1",
    "prettier": "^2.8.8",
    "prettier-plugin-jsdoc": "^0.4.2",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "engines": {
    "npm": ">=8.0.0"
  }
}
